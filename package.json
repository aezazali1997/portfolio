{
  "name": "react-portfolio",
  "version": "0.1.0",
  "private": true,
  "eslintConfig": {
    "extends": "@imaginary-cloud/react"
  },
  "prettier": "@imaginary-cloud/prettier-config",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@craco/craco": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@imaginary-cloud/eslint-config-react": "^1.0.1",
    "@material-ui/core": "^4.11.4",
    "@material-ui/system": "^4.11.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scroll": "^1.8.2",
    "@types/react-typing-effect": "^2.0.1",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@wellyshen/use-web-animations": "^0.7.4",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "clsx": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.28.0",
    "eslint-plugin-react": "^7.24.0",
    "framer-motion": "^11.2.13",
    "history": "^5.0.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mobx-react-form": "^3.1.0",
    "postcss-loader": "4.0.3",
    "postcss-nested": "^6.0.1",
    "react": "^17.0.2",
    "react-animated-cursor": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-jss": "^10.6.0",
    "react-router-dom": "6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-typing-effect": "^2.0.5",
    "tailwind-merge": "^2.3.0",
    "typescript": "^4.1.2",
    "typewriter-effect": "^2.17.0",
    "validatorjs": "^3.22.1",
    "web-vitals": "^1.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "scripts": {
    "start": "craco start --config config/craco.config.js",
    "build": "craco build --config config/craco.config.js",
    "test": "craco test --config config/craco.config.js",
    "eject": "craco eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "isready": "npm run format && npm run lint && npm run test:coverage && npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.11",
    "autoprefixer": "^10.4.19",
    "babel-eslint": "^10.1.0",
    "enzyme-to-json": "^3.6.2",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8.4.39",
    "prettier": "2.3.1",
    "tailwindcss": "3.0",
    "ts-jest": "^27.1.3"
  }
}
